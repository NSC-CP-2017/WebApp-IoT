<% include header.ejs %>
<div id="title-banner">
	<div class="container center">
		<h3 class="header light white-text">My Projects</h3>
		<p id="breadcrumbs"><a href="/">Home</a> &raquo; My Projects </p>
	</div>
</div>
<div class="container">
	<div class="section">
		<div class="row">
			<div class="col s12 m6">
				<ul class="collection with-header">
					<li class="collection-header"><h5 class="orange-text">Projects </h5></li>
					<% for(var i=0; i<result.length; i++) { %>

					<a href='/projects/<%= result[i]["appID"] %>' class="collection-item truncate"><%= result[i]["name"] %></a>

					<% } %>
				</ul>

				<a href="/projects/new" id="download-button" class="btn waves-effect waves-light orange" type="submit"><i class="material-icons left">note_add</i>New Project</a>
			</div>
			<div class="col s12 m6">
				<ul class="collection with-header react">
					<li class="collection-header"><h5 class="blue-text">Smart ReActs</h5></li>
					<% if(typeof reacts != 'undefined') { for(var i=0; i<reacts.length; i++) { %>
					<a href='/reacts/<%= reacts[i]["actID"] %>' class="collection-item truncate"><%= reacts[i]["name"] %></a>
					<% } } %>
				</ul>

				<a href="/reacts/new" id="download-button" class="btn waves-effect waves-light blue" type="submit"><i class="material-icons left">note_add</i>New ReAct</a>
			</div>
		</div>
	</div>
</div>

<% include footer.ejs %>
